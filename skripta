pipeline {
    agent any
    stages{
        stage("checkout"){
            steps{
                script{
                    sh "rm -rf test"
                    sh "git clone https://github.com/Jocko4/test.git"
        
            
                }
            }
       
        }
        stage("checkin"){
            steps{
                script{
                    sh "ansible-playbook -i Inventory/Test Test.yml"
                    sh "ls git2"
        
            
                }
            }
        }
    }
}
